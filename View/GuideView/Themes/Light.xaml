<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <RadialGradientBrush x:Key="GradientBackground">
        <GradientStop Color="#F6F5F2"  Offset="0.0"/>
        <GradientStop Color="#F3D0D7" Offset="1.5"/>
    </RadialGradientBrush>
    <LinearGradientBrush x:Key="LinearBackground">
        <GradientStop Color="#F6F5F2"  Offset="0.3"/>
        <GradientStop Color="#F3D0D7" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Text" Color="Black"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#F0EBE3"/>
    <SolidColorBrush x:Key="BorderBackground" Color="#F6F5F2"/>
    <SolidColorBrush x:Key="ComplexRequestBorder" Color="Black"/>
    <SolidColorBrush x:Key="ListBoxBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="ListBoxBackgrnd" Color="White"/>
    <SolidColorBrush x:Key="NotSelectedButton" Color="DarkGray"/>
    <SolidColorBrush x:Key="SelectedButton" Color="Black"/>
    <SolidColorBrush x:Key="FinalHeaderBorder" Color="Black"/>
    <SolidColorBrush x:Key="CardText" Color="Black"/>

</ResourceDictionary>